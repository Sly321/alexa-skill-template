{
  "buildOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "es5",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitHelpers": true
  }
}
